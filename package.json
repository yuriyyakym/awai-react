{
  "name": "awai-react",
  "version": "v0.0.1-alpha-10",
  "author": "Yuriy Yakym",
  "description": "React hooks for Awai integration",
  "license": "MIT",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && rollup -c rollup.config.js",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yuriyyakym/awai-react.git"
  },
  "bugs": {
    "url": "https://github.com/yuriyyakym/awai-react/issues"
  },
  "keywords": [
    "react",
    "state",
    "library",
    "typescript",
    "javascript",
    "promise",
    "async"
  ],
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.2.3",
    "awai": "^1.0.0-rc-7",
    "jsdom": "^24.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.2.3",
    "rollup": "^3.26.3",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "typescript": "^5.1.6",
    "vitest": "^2.1.0"
  },
  "peerDependencies": {
    "awai": "*",
    "react": "^18.2.0 || ^19.0.0"
  }
}
